{
    "name": "esc-company/view-transformer",
    "description": "View Transformer make profile image or name if client's information is empty.",
    "keywords": [
        "esc-company",
        "php7",
        "development",
        "tdd",
        "profile"
    ],
    "type": "project",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "EscCompany\\ViewTransformer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "start": [
            "@php bin/init.php"
        ],
        "test": [
            "./vendor/bin/phpunit tests"
        ]
    }
}